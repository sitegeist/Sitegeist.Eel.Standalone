{
    "name": "sitegeist/eel-standalone",
    "type": "library",
    "license": ["MIT"],
    "description": "Standalone version of neos/eel without dependency to the neos/flow framework",
    "require": {
        "neos/utility-unicode": "~4.3",
        "neos/utility-objecthandling":"~4.3",
        "psr/simple-cache": "^1.0.0",
        "psr/simple-cache-implementation": "^1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Sitegeist\\Eel\\Standalone\\": "Classes"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Sitegeist\\Eel\\Standalone\\Tests\\": "Tests"
        }
    }
}
